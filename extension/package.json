{
    "name": "vgsasm-vscode-extension",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.1"
    },
    "main": "./extension.js",
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "vgsasm.helloWorld",
                "title": "vgsasm: say hello world"
            }
        ],
        "languages": [
            {
                "id": "vgsasm",
                "extensions": [
                    ".asm",
                    ".inc",
                    ".z80",
                    ".vgs"
                ]
            }
        ],
        "grammars": [
            {
                "language": "vgsasm",
                "scopeName": "source.vgsasm",
                "path": "./vgsasm.tmLanguage.json"
            }
        ]
    }
}